{
  "name": "feathers-hooks-utils",
  "version": "0.1.2",
  "description": "Utility library for writing Feathersjs hooks.",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint --ext .js,.jsx .",
    "build": "rm -rf lib && babel src --presets babel-preset-es2015 --out-dir lib",
    "pretest": "npm run build",
    "test": "mocha --reporter spec",
    "precover": "npm run build",
    "cover": "node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- -R spec test/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eddyystop/feathers-hooks-utils.git"
  },
  "keywords": [
    "feathers",
    "feathersjs",
    "hooks"
  ],
  "author": "John <johnsz9999@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eddyystop/feathers-hooks-utils/issues"
  },
  "homepage": "https://github.com/eddyystop/feathers-hooks-utils#readme",
  "dependencies": {
    "feathers-authentication": "0.7.9"
  },
  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-eslint": "6.1.0",
    "babel-preset-es2015": "6.9.0",
    "chai": "3.5.0",
    "coveralls": "2.11.9",
    "eslint": "3.0.1",
    "eslint-config-airbnb": "9.0.1",
    "eslint-plugin-import": "1.10.2",
    "eslint-plugin-jsx-a11y": "1.5.5",
    "eslint-plugin-react": "5.2.2",
    "istanbul": "0.4.4",
    "mocha": "2.5.3"
  },
  "engines": {
    "node": ">4.2.4"
  }
}
